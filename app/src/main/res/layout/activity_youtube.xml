<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".activitys.CadastroActivity" android:background="@color/cor_tema">

    <view android:layout_width="0dp"
          android:layout_height="300dp"
          class="com.google.android.youtube.player.YouTubePlayerView"
          android:id="@+id/viewYoutubePlayer"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintTop_toTopOf="parent"
          app:layout_constraintStart_toStartOf="parent"/>
    <TextView
            android:text="Determinção de sal em água"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView32"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toBottomOf="@+id/viewYoutubePlayer"
            android:layout_marginStart="16dp" android:layout_marginTop="16dp" android:textSize="22sp"
            android:textColor="@color/white" android:fontFamily="@font/viga"/>
</androidx.constraintlayout.widget.ConstraintLayout>