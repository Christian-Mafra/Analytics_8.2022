<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".fragments.EmbreveFragment">
    <TextView
            android:text="Em breve no\n Analytics"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView19"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/guideline8" app:layout_constraintBottom_toTopOf="@+id/guideline8"
            android:textSize="30sp" android:textColor="@color/cor_tema" android:textStyle="bold"
            android:gravity="center"/>
    <ImageView
            android:src="@drawable/embreve"
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/imageView"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="24dp" android:layout_marginEnd="24dp"
            android:scaleType="fitStart" app:layout_constraintTop_toBottomOf="@+id/textView19"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline8"
                                                android:orientation="horizontal"
                                                app:layout_constraintGuide_percent="0.3"/>
</androidx.constraintlayout.widget.ConstraintLayout>